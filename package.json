{
  "name": "react-outsideclick",
  "version": "1.1.3",
  "description": "A react wrapper component for handling clicks outside target elements",
  "main": "lib/react-outsideclick.js",
  "module": "lib/react-outsideclick.es.js",
  "scripts": {
    "start": "npm run styleguide",
    "build": "npm-run-all --sequential build:lib build:types",
    "build:lib": "rollup -c",
    "build:types": "tsc",
    "prepublish": "npm run build",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "deploy": "npm run styleguide:build && gh-pages -d styleguide",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "everdimension",
  "keywords": [
    "react",
    "react-component",
    "outside click",
    "document click",
    "close on close"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/everdimension/react-outsideclick.git"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@types/react": "^16.9.53",
    "babel-loader": "^8.1.0",
    "gh-pages": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-styleguidist": "^11.1.1",
    "rollup": "^2.32.1",
    "typescript": "^4.0.3",
    "webpack": "^4.6.0"
  }
}
